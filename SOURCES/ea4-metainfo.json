{
   "additional_packages" : [
      "ea-modsec2-rules-owasp-crs",
      "ea-modsec30-rules-owasp-crs",
      "ea-modsec30-connector-nginx",
      "ea-modsec30-connector-apache24",
      "ea-tomcat85",
      "ea-nodejs16",
      "ea-nginx",
      "ea-nginx-brotli",
      "ea-nginx-gzip",
      "ea-nginx-http2",
      "ea-nginx-standalone"
   ],
   "default_directoryindex" : "index.php index.php8 index.php7 index.php5 index.perl index.pl index.plx index.ppl index.cgi index.jsp index.jp index.phtml index.shtml index.xhtml index.html index.htm index.js",
   "default_php_handler" : "cgi",
   "default_php_package" : "ea-php74",
   "default_phpfpm_security_limit_extensions" : ".phtml .php .php3 .php4 .php5 .php6 .php7 .php8"
}
